{
    "env": {
        "browser": true,
        "node":true,
        "es6": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": ["error", "always"],
        "no-cond-assign": ["error", "always"],
        "no-console": "off"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}